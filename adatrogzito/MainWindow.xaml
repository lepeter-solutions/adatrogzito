<Window x:Class="adatrogzito.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:adatrogzito"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="output" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="212,10,10,10"/>
        <Button x:Name="uj" Content="Új hozzáadás" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="28" Width="197" Click="uj_Click"/>
        <Button x:Name="torles" Content="Törlés" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" Height="28" Width="197" Click="torles_Click"/>
        <Button x:Name="import" Content="Import file" HorizontalAlignment="Left" Margin="10,396,0,0" VerticalAlignment="Top" Height="28" Width="197" Click="import_Click"/>
        <Button x:Name="export"  Content="Export file" HorizontalAlignment="Left" Margin="10,363,0,0" VerticalAlignment="Top" Height="28" Width="197" Click="export_Click"/>
    </Grid>
</Window>
